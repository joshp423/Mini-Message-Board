<html>
    <head>
        <title>Mini Messaging App</title>
        <link rel="stylesheet" href="/styles.css">
    </head>
    <body>
        <%- include('navbar', {links: links}) %>
        <div class="messageSection">
            <% messages.forEach((message) => { %>
                <%- include('messages/message', {message: message}); %>
            <% }); %>
        </div>
    </body>
</html>